        android:layout_marginTop="52dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/btn_shap"
        android:drawableStart="@drawable/skin"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="start|center_vertical"
        android:paddingHorizontal="5dp"
        android:text="   Skin"
        android:textColor="#fff"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/lung" />

    <Button
        android:id="@+id/blood"
        android:layout_width="155dp"
        android:layout_height="59dp"

        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="56dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/btn_shap"
        android:drawableStart="@drawable/blood"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="start|center_vertical"
        android:paddingHorizontal="5dp"
        android:text="  BLood"
        android:textColor="#fff"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/skin" />

    <Button
        android:id="@+id/breast"
        android:layout_width="155dp"
        android:layout_height="59dp"

        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="52dp"
        android:background="@drawable/btn_shap"
        android:drawableStart="@drawable/breast"
        android:fontFamily="@font/poppins_semibold"
        android:gravity="start|center_vertical"
        android:paddingHorizontal="5dp"
        android:text="Breast"
        android:textColor="#fff"
        android:textSize="20dp"
